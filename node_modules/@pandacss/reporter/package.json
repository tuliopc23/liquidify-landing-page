{
  "name": "@pandacss/reporter",
  "version": "1.3.0",
  "description": "Track and report usage of tokens and recipes",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Segun Adebayo <joseshegs@gmail.com>",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "homepage": "https://panda-css.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chakra-ui/panda.git",
    "directory": "packages/reporter"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "table": "6.9.0",
    "wordwrapjs": "5.1.0",
    "@pandacss/core": "1.3.0",
    "@pandacss/generator": "1.3.0",
    "@pandacss/logger": "1.3.0",
    "@pandacss/shared": "1.3.0",
    "@pandacss/types": "1.3.0"
  },
  "devDependencies": {
    "markdown-table": "3.0.4"
  },
  "scripts": {
    "build": "tsup src/index.ts --format=esm,cjs --dts",
    "build-fast": "tsup src/index.ts --format=esm,cjs --no-dts",
    "dev": "pnpm build-fast --watch"
  }
}