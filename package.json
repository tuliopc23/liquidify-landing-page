{
  "name": "liquidify-landing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "bun@1.2.21",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.2.0"
  },
  "scripts": {
    "predev": "bun run panda:once",
    "dev": "vite",
    "build": "vite build",
    "prebuild": "bun run panda:once",
    "preview": "vite preview --host",
    "lint": "eslint src scripts *.config.*",
    "lint:fix": "eslint src scripts *.config.* --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src public scripts *.config.* *.html *.json *.md",
    "format:check": "prettier --check src public scripts *.config.* *.html *.json *.md",
    "check": "bun run lint && bun run typecheck && bun run format:check",
    "fix": "bun run lint:fix && bun run format",
    "clean": "rm -rf dist .output .tanstack .nitro .vinxi",
    "panda": "panda --watch",
    "panda:once": "panda",
    "verify:panda": "node scripts/verify-panda-migration.mjs"
  },
  "dependencies": {
    "@react-spring/web": "^10.0.3",
    "@tanstack/react-router": "^1.131.44",
    "@use-gesture/react": "^10.3.1",
    "cache": "^3.0.0",
    "lucide-react": "^0.544.0",
    "prismjs": "^1.30.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@pandacss/dev": "^1.3.0",
    "@tanstack/router-plugin": "^1.131.44",
    "@types/node": "^24.5.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.5"
  }
}
