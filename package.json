{
  "name": "liquidify-landing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "bun@1.2.21",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --host",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "bun run lint && bun run typecheck && bun run format:check",
    "fix": "bun run lint:fix && bun run format",
    "clean": "rm -rf dist .output .tanstack .nitro .vinxi",
    "panda": "panda --watch",
    "panda:once": "panda",
    "verify:panda": "node scripts/verify-panda-migration.mjs"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.131.41",
    "prismjs": "^1.30.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@pandacss/dev": "^1.3.0",
    "@tanstack/router-plugin": "^1.131.43",
    "@types/node": "^24.4.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5"
  }
